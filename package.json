{
  "name": "vue-animated-counter",
  "version": "0.2.0",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-animated-counter.umd.js",
  "module": "./dist/vue-animated-counter.es.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/vue-animated-counter.es.js",
      "require": "./dist/vue-animated-counter.umd.js"
    },
    "./dist/style.css": "./dist/vue-animated-counter/style.css"
  },
  "types": "./dist/types/components/AnimatedCounter.vue.d.ts",
  "keywords": [
    "vue3",
    "typescript",
    "animation"
  ],
  "private": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "preserve": "vite build",
    "serve": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "exit 0",
    "type-check": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.2.47"
  },
  "dependencies": {
    "@vueuse/components": "^10.1.2",
    "@vueuse/core": "^10.1.2",
    "rollup-plugin-typescript2": "^0.34.1"
  },
  "devDependencies": {
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.2.0"
  }
}